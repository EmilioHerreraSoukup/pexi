{
  "extends": ["airbnb", "plugin:prettier/recommended"],
  "plugins": ["emotion", "prettier","react","react-hooks"],
  "env": {
    "jest": true,
    "browser": true
  },
  "rules": {
    "prettier/prettier": ["error"],
    "import/prefer-default-export": "off",
    "comma-dangle": ["error", "never"]
  }
}
